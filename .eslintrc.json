{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "no-underscore-dangle": 0,
        "max-len": ["error", { "code": 150 }],
        "operator-linebreak": 0,
        "comma-dangle": 0,
        "implicit-arrow-linebreak": 0
    }
}
